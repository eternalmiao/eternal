package cn.bug.bean;

/**
 * 订单和评论的组合
 * @author ffpff
 *
 */
public class GOrderAndComm
{
	private GOrder order;
	private Command comm;
	public GOrderAndComm(GOrder order, Command comm)
	{
		super();
		this.order = order;
		this.comm = comm;
	}
	public GOrderAndComm()
	{
		super();
	}
	public GOrder getOrder()
	{
		return order;
	}
	public void setOrder(GOrder order)
	{
		this.order = order;
	}
	public Command getComm()
	{
		return comm;
	}
	public void setComm(Command comm)
	{
		this.comm = comm;
	}
	
}

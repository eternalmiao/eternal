package cn.bug.bean;

/**
 * 用户收藏表
 * @author 远朋
 *
 */
public class Wishlist {
	private int wid;		//收藏编号
	private Goods goods;	//商品信息
	private ShopUser user;	//用户信息
	
	public Wishlist() {
		super();
	}
	public Wishlist(int wid, Goods goods, ShopUser user) {
		super();
		this.wid = wid;
		this.goods = goods;
		this.user = user;
	}
	public int getWid() {
		return wid;
	}
	public void setWid(int wid) {
		this.wid = wid;
	}
	public Goods getGoods() {
		return goods;
	}
	public void setGoods(Goods goods) {
		this.goods = goods;
	}
	public ShopUser getUser() {
		return user;
	}
	public void setUser(ShopUser user) {
		this.user = user;
	}
	
}
